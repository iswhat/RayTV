{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "targetSdkVersion": "5.1.1(19)",
        "compatibleSdkVersion": "5.1.1(19)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          },
          "aot": {
            "enable": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
        "debuggable": true,
        "compileMode": "debug"
      },
      {
        "name": "release",
        "debuggable": false,
        "compileMode": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "raytv",
      "srcPath": "./raytv",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ],
          "runtimeOS": "HarmonyOS"
        },
        {
          "name": "ohosTest",
          "applyToProducts": [
            "default"
          ],
          "runtimeOS": "HarmonyOS"
        }
      ]
    }
  ]
