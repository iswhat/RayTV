      "urls": [
        "http://127.0.0.1:7890"
      ]
    },
    {
      "name": "全局",
      "hosts": [
        ".*."
      ],
      "urls": [
        "socks5://127.0.0.1:7891"
      ]
    }
  ],
  "rules": [
    {
      "hosts": [
        "api.52wyb.com"
      ],
      "regex": [
        "m3u8?pt=m3u8"
      ],
      "exclude": [
        "test.json"
      ]
    },
    {
      "hosts": [
        "www.maolvys.com"
      ],
      "script": [
        "document.getElementsByClassName('swal-button swal-button--confirm')[0].click()"
      ]
    }
  ],
  "headers": [
    {
      "host": "gslbserv.itv.cmvideo.cn",
